{
    "configurations": [
        {
            "name": "AVR",
            "includePath": [
                "/usr/lib/avr/include/avr/*"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/avr-gcc",
            "cStandard": "c89",
            "cppStandard": "c++98",
            "intelliSenseMode": "linux-gcc-x86",
            "mergeConfigurations": false,
            "browse": {
                "path": [
                    "/usr/lib/avr/include/**",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}